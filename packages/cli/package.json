{
  "name": "@culur/culur",
  "version": "0.0.0",
  "description": "Sharing Typescript configurations",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/culur/culur.git"
  },
  "bugs": {
    "url": "https://github.com/culur/culur/issues"
  },
  "homepage": "https://github.com/culur/culur#readme",
  "author": "Phạm Hồng Phúc <phamhongphuc.info@gmail.com>",
  "license": "MIT",
  "keywords": [
    "config",
    "typescript"
  ],
  "main": "lib/main/index.js",
  "types": "lib/main/index.d.ts",
  "module": "lib/module/index.js",
  "files": [
    "tsconfig.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "babel-node --extensions .ts -- src/index.ts",
    "build": "run-s build:*",
    "build:tsc": "tsc",
    "build:babel": "babel src --extensions .ts --out-dir lib --source-maps inline",
    "clean": "run-s clean:*",
    "clean:build": "rimraf build",
    "start": "node lib/index.js"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.16",
    "babel-plugin-module-resolver": "^4.1.0",
    "chalk": "^4.1.0",
    "commander": "^7.1.0",
    "fs-extra": "^9.1.0",
    "lodash": "^4.17.21",
    "prompts": "^2.4.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@babel/node": "^7.12.16",
    "@babel/register": "^7.12.13",
    "@types/fs-extra": "^9.0.7",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.28",
    "@types/prompts": "^2.0.9"
  }
}
